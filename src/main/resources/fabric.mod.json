{
  "schemaVersion": 1,
  "id": "unnamed_mod",
  "version": "${version}",

  "name": "Example Mod",
  "description": "This is an unnamed_mod description! Tell everyone what your mod is about!",
  "authors": [
    "Me!"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-unnamed_mod-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/unnamed_mod/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "net.fabricmc.unnamed_mod.UnnamedMod"
      }
    ]
  },
  "mixins": [
    "unnamed_mod.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.13.3",
    "fabric": "*",
    "minecraft": "1.19.x",
    "java": ">=17",
    "fabric-language-kotlin": ">=1.7.1+kotlin.1.6.10"
  },
  "suggests": {
    "another-mod": "*"
  }
}
